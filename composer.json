{
    "name": "genericmilk/kaboom",
    "description": "Make simple API calls from Laravel!",
    "keywords": ["laravel", "api", "curl", "get", "post"],
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Day",
            "email": "peterday.main@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3"
    },
    "autoload": {
        "psr-4": {
            "Genericmilk\\Kaboom\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        },
        "laravel": {
            "providers": [
                "Genericmilk\\Kaboom\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^8.0.0",
        "nexmo/client": "^2.4"
    }
}
